{
  "kind": "collectionType",
  "collectionName": "tasks",
  "info": {
    "singularName": "task",
    "pluralName": "tasks",
    "displayName": "Task"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "taskId": {
      "type": "uid"
    },
    "taskName": {
      "type": "string"
    },
    "taskStatus": {
      "type": "enumeration",
      "enum": [
        "todo",
        "done",
        "late",
        "deleted"
      ]
    },
    "taskDate": {
      "type": "datetime"
    },
    "taskMember": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::member.member"
    },
    "taskCreator": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::member.member"
    }
  }
}
